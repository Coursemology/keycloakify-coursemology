    "lint-eslint": "eslint . --ext .ts --ext .tsx",
    "lint-prettier": "prettier \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run lint-eslint --cache && yarn run lint-prettier --check",
    "lint-check": "yarn run lint-eslint && yarn run lint-prettier --check",
    "lint-fix": "yarn run lint-eslint --fix  && yarn run lint-prettier --write"
